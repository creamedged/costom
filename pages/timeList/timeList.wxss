/* pages/timeList/timeList.wxss */
.container{margin-top: 25px!important; padding-bottom:80rpx;}
.headerBox .header_content {color:#222;font-weight:bold;}

.header{background: #fff; line-height: 100rpx; padding:0 30rpx; font-size:34rpx; overflow: hidden;text-overflow:ellipsis;white-space: nowrap; }
.timeSelected{background: #fff;  overflow: auto; padding:50rpx 0 100rpx; text-align: center;}
.timeSelected::-webkit-scrollbar {display:none}
.timeSelected .item{width:160rpx; height:120rpx; padding-top:40rpx; margin:0 25rpx; border-radius:100rpx; position: relative; border:1px solid #d8d8d8; display: inline-block; }
.timeSelected .item text{display: block; text-align: center; }
.timeSelected .item .week{font-size:30rpx;}
.timeSelected .item .date{font-size:26rpx;}
.timeSelected .item .icon{position: absolute;left:0px; right:0px; text-align: center;bottom:-70rpx;}
.timeSelected .active{color:#fff; background:#00a0e9}
.timeSelected .active .icon{color:#00a0e9}
.distance {height:64rpx; line-height: 64rpx; background: #f2f2f2;margin-bottom:34rpx;font-size:28rpx;padding-left:30rpx;color:#939393;}
.bespeakbox {width:92%;margin:0 auto;font-size:28rpx;border-top:1px solid #ececec}
.bespeak{width:100%;height: 100rpx; padding:0 30rpx; border:1px solid #ececec;box-sizing:border-box; display:block; line-height: 100rpx; border-top:0}
.bespeak .btn{width:86rpx;height:98rpx;float:right;}
.noBes{ color: #e7e7e7;}
.bespeak .besBtn{background:url(data:image/png;base64,) no-repeat right center;background-size: 50%}
.active {color:#ff9e05; border:1px solid #ff9e05;margin-top:-1rpx;}
.active .besBtn{background:url(data:image/png;base64,) no-repeat right center;background-size: 50%}

.selectedTime{position: fixed; bottom:30rpx; left:0; right:0; margin:0 30rpx; height: 80rpx; line-height: 80rpx; text-align: center; color: #fff; background: #c6c6c6; font-size: 28rpx; border-radius:10rpx;}
.enselectedTime {background: #4396e9}