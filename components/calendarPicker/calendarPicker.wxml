<view class="calendar-picker static"><view class="calendar-picker-box"><view class="calendar-picker-header"><view class="title-date">{{title}}</view></view><view class="calendar-picker-week"><text class="week-cell">日</text><text class="week-cell">一</text><text class="week-cell">二</text><text class="week-cell">三</text><text class="week-cell">四</text><text class="week-cell">五</text><text class="week-cell">六</text></view><swiper current="{{current}}" style="height: {{swiperHeight || '60vw'}}" bindchange="change"><block wx:for="{{months}}" wx:for-index="monthIndex" wx:for-item="month" wx:key="title"><swiper-item><view class="calendar-picker-month"><view class="date-cell {{date.disabled ? 'disabled' : ''}} {{date.fullDate ? 'disabled' : ''}} {{date.isPrevMonth ? 'prev-month-day' : ''}} {{date.isNextMonth ? 'next-month-day' : ''}} {{date.date == currentDate ? 'active' : ''}} {{date.date == todayDate ? 'date-today' : ''}} " wx:for="{{month.dates}}" wx:for-index="dateIndex" wx:for-item="date" wx:key="date" data-disabled="{{date.disabled}}" data-datestr="{{date.date}}" bindtap="tapDate">{{date.day}}</view></view></swiper-item></block></swiper></view></view>